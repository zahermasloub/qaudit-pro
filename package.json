{
  "name": "qaudit-pro",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start -p 3001",
    "prepare": "husky install",
    "lint": "eslint . --ext .ts,.tsx",
    "fix": "eslint . --ext .ts,.tsx --fix && prettier --write .",
    "cc:scan": "tsx scripts/clean-code-scan.ts",
    "cc:refactor": "tsx scripts/clean-code-refactor.ts",
    "cc:report": "tsx scripts/clean-code-report.ts",
    "cc:all": "npm run cc:scan && npm run cc:refactor && npm run cc:report",
    "audit:s7": "tsx scripts/sprint7-presence-audit.ts",
    "db:setup": "prisma db push && npm run seed",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "seed": "tsx scripts/seed.ts",
    "test:s7": "tsx scripts/sprint7-e2e.test.ts",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.908.0",
    "@aws-sdk/s3-request-presigner": "^3.908.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@hookform/resolvers": "^3.10.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "5.19.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/node-cron": "^3.0.11",
    "@types/react-dropzone": "^4.2.2",
    "@types/tesseract.js": "^0.0.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.0.0",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.453.0",
    "next": "14.2.5",
    "next-auth": "^4.24.11",
    "node-cron": "^4.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.52.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-rtl": "^0.9.0",
    "tesseract.js": "^6.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "autoprefixer": "10.4.17",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.2.0",
    "husky": "^9.1.7",
    "jscodeshift": "^17.3.0",
    "lint-staged": "^16.2.4",
    "madge": "^8.0.0",
    "postcss": "8.4.31",
    "prettier": "^3.6.2",
    "prisma": "5.19.0",
    "tailwindcss": "3.4.10",
    "ts-morph": "^27.0.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "5.4.2"
